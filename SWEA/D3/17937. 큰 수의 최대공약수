#include<iostream>

using namespace std;

int main(int argc, char** argv)
{
	int test_case;
	int T;
	cin>>T;
	
	for(test_case = 1; test_case <= T; ++test_case)
	{

		/////////////////////////////////////////////////////////////////////////////////////////////
	    	string GCD = "";
		string a,b;
	    	cin >> a >> b;
			
	    	if(a==b){ GCD = a; }
    		else{ GCD = "1"; }
			
	    	cout << "#" << test_case << " " << GCD << "\n";
		/////////////////////////////////////////////////////////////////////////////////////////////


	}
	return 0;
}
